# Cluster and RPC
CLUSTER=devnet
RPC_URL=https://api.devnet.solana.com

# Signing keypair for the engine
SIGNER_KEYPAIR_PATH=./secrets/engine-keypair.json

# Token mints and vaults
LIP_MINT=ReplaceWithLipMintPubkey
FEE_MINT=So11111111111111111111111111111111111111112
FEE_VAULT=ReplaceWithFeeVaultPubkey

# Constraints
MIN_FEE_BALANCE=0.05
MAX_BUY_AMOUNT=0.02
COOLDOWN_SECONDS=600
SLIPPAGE_BPS=150

# Optional random window to reduce predictability
RANDOMIZE_WINDOW_SECONDS=120

# Dry run does everything except send tx
DRY_RUN=true

# Logging
LOG_LEVEL=info
